{
  "name": "@servicestack/react",
  "version": "2.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/ServiceStack/servicestack-react"
  },
  "private": false,
  "files": [
    "dist"
  ],
  "type": "module",
  "module": "./dist/servicestack-react.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/servicestack-react.mjs",
      "require": "./dist/servicestack-react.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only build-style",
    "build-style": "postcss src/tailwind.css -o dist/styles.css",
    "preview": "vite preview",
    "build-only": "vite build -l error",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "update-deps": "npx npm-check-updates -u && npm install",
    "release": "bump patch --commit --push --tag && npm publish"
  },
  "dependencies": {
    "@servicestack/client": "^2.1.13",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.28.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.10",
    "@types/node": "^24.6.2",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/browser": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.56.1",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "shx": "^0.4.0",
    "tailwindcss": "^3.4.18",
    "typescript": "~5.9.3",
    "uglify-js": "^3.19.3",
    "vite": "^7.1.7",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4",
    "webdriverio": "^9.20.0"
  },
  "overrides": {
    "@microsoft/api-extractor": {
      "typescript": "$typescript"
    }
  }
}