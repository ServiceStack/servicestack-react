{
  "name": "@servicestack/react",
  "version": "2.0.14",
  "repository": {
    "type": "git",
    "url": "https://github.com/ServiceStack/servicestack-react"
  },
  "private": false,
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/servicestack-react.umd.cjs",
  "module": "./dist/servicestack-react.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/servicestack-react.mjs",
      "require": "./dist/servicestack-react.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only build-style && run-p build-minify",
    "build-minify": "uglifyjs dist/servicestack-react.mjs --compress --mangle --webkit -o dist/servicestack-react.min.mjs",
    "build-style": "postcss src/tailwind.css -o dist/styles.css",
    "build-copy": "npm run build && npm run copy-react-spa && npm run copy-react-vite",
    "copy-react-spa": "rm /home/mythz/src/NetCoreTemplates/react-spa/MyApp.Client/node_modules/@servicestack/react/dist/* && cp ./dist/* /home/mythz/src/NetCoreTemplates/react-spa/MyApp.Client/node_modules/@servicestack/react/dist/ && rm -rf /home/mythz/src/NetCoreTemplates/react-spa/MyApp.Client/node_modules/.vite",
    "copy-react-vite": "rm /home/mythz/src/NetCoreTemplates/react-vite/MyApp.Client/node_modules/@servicestack/react/dist/* && cp ./dist/* /home/mythz/src/NetCoreTemplates/react-vite/MyApp.Client/node_modules/@servicestack/react/dist/ && rm -rf /home/mythz/src/NetCoreTemplates/react-vite/MyApp.Client/node_modules/.vite",
    "preview": "vite preview",
    "build-only": "vite build -l error",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "update-deps": "npx npm-check-updates -u && npm install",
    "bump": "npm version patch && git push origin main --follow-tags",
    "bump:minor": "npm version minor && git push origin main --follow-tags",
    "bump:major": "npm version major && git push origin main --follow-tags",
    "release": "gh release create v$(node -p \"require('./package.json').version\") --title v$(node -p \"require('./package.json').version\") --generate-notes"
  },
  "dependencies": {
    "@servicestack/client": "^2.1.13"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "react-router-dom": "^7.0.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.1.17",
    "@tailwindcss/typography": "^0.5.19",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@vitejs/plugin-react": "^5.1.0",
    "@vitest/browser": "^4.0.7",
    "@vitest/browser-playwright": "^4.0.7",
    "autoprefixer": "^10.4.21",
    "jsdom": "^27.1.0",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.56.1",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.5",
    "react-syntax-highlighter": "^16.1.0",
    "shx": "^0.4.0",
    "tailwindcss": "^4.1.17",
    "typescript": "~5.9.3",
    "uglify-js": "^3.19.3",
    "vite": "^7.2.1",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.7",
    "webdriverio": "^9.20.0"
  }
}
